{
  "name": "aether-dex",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "bun@1.2.20",
  "scripts": {
    "dev": "bun run next dev",
    "build": "bun run next build",
    "start": "bun run next start",
    "lint": "bun run oxlint ./app",
    "type-check": "bun run tsc --noEmit",
    "test": "bun run vitest --bail=1",
    "test:ui": "bun run vitest --ui",
    "test:run": "bun run vitest run --bail=1",
    "test:watch": "bun run vitest --bail=1",
    "test:watch:fast": "bun run vitest --bail=1 --reporter=dot",
    "test:coverage": "bun run vitest run --coverage",
    "test:coverage:ui": "bun run vitest --ui --coverage",
    "test:coverage:watch": "bun run vitest --coverage",
    "update-deps": "bun update",
    "clean": "rm -rf .next out node_modules"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "^15.4.6",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.9.2",
    "@wagmi/core": "^2.19.0",
    "viem": "^2.33.3",
    "@tanstack/react-query": "^5.85.3",
    "@web3modal/wagmi": "^5.1.11",
    "wagmi": "^2.16.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^2.1.9",
    "jsdom": "^25.0.1",
    "oxlint": "^0.15.15",
    "vitest": "^3.2.4"
  },
  "trustedDependencies": [
    "oxlint",
    "next"
  ],
  "engines": {
    "node": ">=24.0.0",
    "bun": ">=1.2.0"
  }
}
